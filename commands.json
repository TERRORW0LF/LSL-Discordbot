{
    "commandList": [
        {
            "group": "basic",
            "name": "help",
            "regex": "^(.*)\\s?(help|commands)$",
            "help": "help / <command-group> help",
            "path": "Commands/Basic/help.js"
        },
        {
            "group": "basic",
            "name": "remind me",
            "regex": "^(remindme|remind me|set reminder|reminder) ((0[1-9]|1[012])/(0[1-9]|[12]\\d|3[01])/\\d{4}( ([01]\\d|2[0123]):[0-5]\\d:[0-5]\\d)?|((\\d+)(weeks?|ws?))?((\\d+)(days?|ds?))?((\\d+)(hours?|hs?))?((\\d+)(mins?|ms?))?((\\d+)(secs?|ss?))?) (.+)",
            "help": "remind me <date (mm/dd/yyyy [hh:mm:ss])|time ([w][d][h][m][s])> <reminder>",
            "path": "Commands/Basic/remindMe.js"
        },
        {
            "group": "basic",
            "name": "reminders",
            "regex": "^(list reminders|show reminders|reminders)$",
            "help": "reminders",
            "path": "Commands/Basic/reminders.js"
        },
        {
            "group": "moderation",
            "name": "mute",
            "regex": "^(mute) <@!?(\\d{17,19})>\\s*((\\d+)(weeks?|ws?))?((\\d+)(days?|ds?))?((\\d+)(hours?|hs?))?((\\d+)(mins?|ms?))?((\\d+)(secs?|ss?))?(.+)?",
            "help": "mute <mention> <duration> [<reason>]",
            "path": "Commands/Moderation/mute.js"
        },
        {
            "group": "moderation",
            "name": "unmute",
            "regex": "^(unmute) <@!?(\\d{17,19})>\\s*(.+)?",
            "help": "unmute <mention> [<reason>]",
            "path": "Commands/Moderation/unmute.js"
        },
        {
            "group": "moderation",
            "name": "kick",
            "regex": "^(kick) <@!?(\\d{17,19})>\\S*(.+)?",
            "help": "kick <mention> [<reason>]",
            "path": "Commands/Moderation/kick.js"
        },
        {
            "group": "moderation",
            "name": "ban",
            "regex": "^(ban) <@!?(\\d{17,19})>\\s*(.+)?",
            "help": "ban <mention> [<reason>]",
            "path": "Commands/Moderation/ban.js"
        },
        {
            "group": "moderation",
            "name": "unban",
            "regex": "^(unban) (.+#?\\d*)(\\s*reason (.+))?",
            "help": "unban <username> [<reason>]",
            "path": "Commands/Moderation/unban.js"
        },
        {
            "group": "moderation",
            "name": "reaction role new",
            "regex": "^(reaction role new|reactionrole new|rr new)$",
            "help": "rr new",
            "path": "Commands/Moderation/reactionRoleNew.js"
        },
        {
            "group": "moderation",
            "name": "reaction role add",
            "regex": "^(reaction role add|reactionrole add|rr add) (https://discordapp.com/channels/\\d+/\\d+/\\d+ )?(\\p{Emoji_Presentation}|\\p{Extended_Pictographic}|<:\\S+:\\d+>) <@&(\\d+)>(\\s*.+)?",
            "help": "rr add [<message>] <emoji> <role> [<explanation>]",
            "path": "Commands/Moderation/reactionRoleAdd.js"
        },
        {
            "group": "fun",
            "name": "dad joke",
