{
    "commandList": [
        {
            "group": "basic",
            "name": "help",
            "regex": "^(.*)\\s?(help|commands)$",
            "help": "help / <command-group> help",
            "path": "Commands/Basic/help.js"
        },
        {
            "group": "basic",
            "name": "remind me",
            "regex": "^(remind\\s*me|set\\s*reminder|reminder) ((0[1-9]|1[012])/(0[1-9]|[12]\\d|3[01])/\\d{4}( ([01]\\d|2[0123]):[0-5]\\d:[0-5]\\d)?|((\\d+)(weeks?|ws?))?((\\d+)(days?|ds?))?((\\d+)(hours?|hs?))?((\\d+)(mins?|ms?))?((\\d+)(secs?|ss?))?) (.+)",
            "help": "remind me <date (mm/dd/yyyy [hh:mm:ss])|time ([w][d][h][m][s])> <reminder>",
            "path": "Commands/Basic/remindMe.js"
        },
        {
            "group": "basic",
            "name": "reminders",
            "regex": "^(list\\s*reminders|show\\s*reminders|reminders)$",
            "help": "reminders",
            "path": "Commands/Basic/reminders.js"
        },
        {
            "group": "moderation",
            "name": "mute",
            "regex": "^(mute)\\s+<@!?(\\d{17,19})>\\s*((\\d+)(weeks?|ws?))?((\\d+)(days?|ds?))?((\\d+)(hours?|hs?))?((\\d+)(mins?|ms?))?((\\d+)(secs?|ss?))?\\s*(.+)?",
            "help": "mute <mention> <duration ([w][d][h][m][s])> [<reason>]",
            "path": "Commands/Moderation/mute.js"
        },
        {
            "group": "moderation",
            "name": "unmute",
            "regex": "^(unmute)\\s+<@!?(\\d{17,19})>\\s*(.+)?",
