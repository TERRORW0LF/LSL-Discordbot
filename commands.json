            "path": "Commands/Lookup/incomplete.js"
        },
        {
            "group": "submit",
            "name": "submit",
            "regex": "(submit|sub)\\s+(\\d+)\\s+(\\S+)\\s+(.+)\\s+(\\d+\\.\\d{2})\\s+(https://\\S+)$",
            "help": "submit <season> <mode> <map> <time> <proof>",
            "path": "Commands/Submit/submit.js"
        },
        {
            "group": "submit",
