            "path": "Commands/Lookup/place.js"
        },
        {
            "group": "lookup",
            "name": "leaderboard",
            "regex": "(lbs?|leader\\s*boards?|leader-boards?|top)\\s+(\\d+)\\s+(\\S+)\\s+(.+)",
