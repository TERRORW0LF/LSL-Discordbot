            "path": "Commands/Submit/deleteMap.js"
        },
        {
            "group": "tournament",
            "name": "sign-in",
            "regex": "^(tournament|tourney)\\s+(sign-in|sign\\s*in)\\s+(^[^0-9][^#\\s]{2,11}#[0-9]{4,6})\\s+(na\\s*east|na\\s*e|na\\s*west|na\\s*w|eu|sa|asia|oce)\\s+(@.+)\\s+(\\d+)$",
            "help": "tournament sign-in <Blizzard battle-tag> <region> <highest role> <hours played>",
            "path": "Commands/Tournament/sign-in.js"
        },
        {
            "group": "tournament",
            "name": "create",
            "regex": "^(tournament|tourney)\\s+(create)\\s+('.+')\\s+((0[1-9]|1[012])/(0[1-9]|[12]\\d|3[01])/\\d{4}\\s+([01]\\d|2[0123]):[0-5]\\d:[0-5]\\d)\\s+(min:\\s*(\\d+))?\\s+(max:\\s*(\\d+))?$",
            "help": "tournament create <name> <date (mm/dd/yyyy hh:mm:ss)> [<min: d>] [<max: d>]",
            "path": "Commands/Tournament/create.js"
