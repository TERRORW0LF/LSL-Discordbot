        {
            "group": "moderation",
            "name": "reaction role add",
            "regex": "^(reaction\\s*role\\s*add|rr\\s*add) (https://discordapp.com/channels/\\d+/\\d+/\\d+\\s+)?(\\p{Emoji_Presentation}|\\p{Extended_Pictographic}|<:\\S+:\\d+>)\\s+<@&(\\d+)>\\s*(.+)?",
